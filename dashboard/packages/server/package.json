{
  "name": "@smartgarden-dashboard/server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev -r dotenv/config --no-notify --respawn src/server dotenv_config_path=../../../.env",
    "start:prod": "ts-node src/server",
    "dev": "yarn start:dev",
    "prod": "yarn start:prod",
    "clean": "rm -rf dist",
    "build": "yarn -s clean && tsc"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.9.28",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "dotenv": "^8.2.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  }
}
